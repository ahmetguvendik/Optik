@model Optik.DTO.ContactDTOs.CreateContactDto

<div id="contact" class="contact">
    <div class="container">
        <div class="row">
            <div class="col-md-6">
                <form class="main_form" asp-controller="Default" asp-action="CreateContact" method="post">
                    <div class="row">
                        <div class="col-md-12 ">
                            <h3>Contact Us</h3>
                        </div>
                        <div class="col-md-12 ">
                            <input class="contactus" placeholder="Name"  asp-for="Name">
                        </div>
                        <div class="col-md-12">
                            <input class="contactus" placeholder="Phone Number" asp-for="PhoneNumber" >
                        </div>
                        <div class="col-md-12">
                            <input class="contactus" placeholder="Email" asp-for="Email">
                        </div>
                        <div class="col-md-12">
                            <input class="contactusmess" placeholder="Message" asp-for="Message">
                        </div>
                        <div class="col-md-12">
                            <button class="send_btn">Send</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div class="container-fluid">
        <div class="map_section">
            <div id="map">
            </div>
        </div>
    </div>
</div>